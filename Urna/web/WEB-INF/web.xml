<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <filter>
        <filter-name>Router</filter-name>
        <filter-class>br.com.votar.router.Router</filter-class>
        <init-param>
            <param-name>idEleicao</param-name>
            <param-value>0</param-value>
        </init-param>
    </filter>
    <filter-mapping>
        <filter-name>Router</filter-name>
        <url-pattern>/eleicao/</url-pattern>
    </filter-mapping>
    <servlet>
        <servlet-name>verificarLogin</servlet-name>
        <servlet-class>br.com.votar.servlet.verificarLogin</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CadastroEleicao</servlet-name>
        <servlet-class>br.com.votar.servlet.CadastroEleicao</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CadastroCandidato</servlet-name>
        <servlet-class>br.com.votar.servlet.CadastroCandidato</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>logout</servlet-name>
        <servlet-class>br.com.votar.servlet.logout</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Votar</servlet-name>
        <servlet-class>br.com.votar.servlet.Votar</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>verificarLogin</servlet-name>
        <url-pattern>/verificarLogin</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CadastroCandidato</servlet-name>
        <url-pattern>/CadastroCandidato</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>logout</servlet-name>
        <url-pattern>/logout</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Votar</servlet-name>
        <url-pattern>/eleicao/votar</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CadastroEleicao</servlet-name>
        <url-pattern>/CadastroEleicao</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
